<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>




<div class="modal fade" id="userTransDetail" tabindex="-1" style="z-index: 2000;"
				role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" >
				<div class="modal-dialog modal-lg">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal"
								aria-hidden="true">×</button>
							<h4 class="modal-title" id="userTransDetailDivLabel">Registered
								Users</h4>
						</div>
						<div class="modal-body" style=" overflow-x:auto;overflow-y:scroll; height: 600px;">
							
							
							
							
							
			<div class="row" id="userInfoDiv">
				<div class="col-lg-12">
					<div class="panel panel-default">
						<div class="panel-heading">User Information</div>
						<!-- /.panel-heading -->
						<div class="panel-body">
							<div class="table-responsive" id="tableArea"></div>
							<!-- /.table-responsive -->
						</div>
						<!-- /.panel-body -->
					</div>
					<!-- /.panel -->
				</div>
			</div>
			<div class="row" id="transInfoDiv">
				<div class="col-lg-12">
					<div class="panel panel-default">
						<div class="panel-heading">Transaction Statistics</div>
						<!-- /.panel-heading -->
						<div class="panel-body">
							<div class="table-responsive" id="tableTxnArea"></div>
							<!-- /.table-responsive -->
						</div>
						<!-- /.panel-body -->
					</div>
					<!-- /.panel -->
				</div>
			</div>




			<div class="row">
				<div class="col-lg-12">
					<div class="panel panel-default">
						<div class="panel-heading">Transactions List</div>
						<!-- /.panel-heading -->
						<div class="panel-body">
							<table width="100%"
								class="table table-striped table-bordered table-hover"
								id="transList">
								<thead>
									<tr>
										<th>TXN ID</th>
										<th title="Gives money to payee">Payer Name</th>
										<th title="Receive money from payer">Payee Name</th>
										<th>Payment Status</th>
										<th>Cash Received</th>
										<th>Sky Credit</th>
										<th>Bill Amount</th>
										<th>GST Enabled</th>
										<th>TXN Type</th>
										<th>TXN Date</th>
										<th>Action Taken Date</th>
									</tr>
								</thead>
								<tbody id="transTableBody" style="overflow: hidden;">
									
								</tbody>
							</table>
							<!-- /.table-responsive -->

						</div>
						<!-- /.panel-body -->
					</div>
					<!-- /.panel -->
				</div>
				<!-- /.col-lg-12 -->
			</div>























			<!-- /.row -->
			<div class="row" id="associatedUsersDiv">
				<div class="col-lg-12">
					<div class="panel panel-default">
						<div class="panel-heading" id="userListHeadingDiv">Associated
							Users</div>
						<!-- /.panel-heading -->
						<div class="panel-body">
							<table width="100%"
								class="table table-striped table-bordered table-hover"
								id="associatedUsers">
								<thead>
									<tr>
										<th>Sr No.</th>
										<th>EZKhata Name</th>
										<th>Mapped Name</th>
										<th>Phone Number</th>
										<th>Balance</th>
										<th>is Active</th>
										<th>Relation</th>
										<th>User Type</th>
										<th>Mapped Date</th>

									</tr>
								</thead>

							</table>
